{
    "name": "differentiatedCareProgramBase",
    "version": "1.0",
    "tag": "",
    "description": "",
    "uses": [
        {
            "name": "differentiatedCareWeightDataset",
            "version": "1.0",
            "type": "dataset_def"
        },
        {
            "name": "differentiatedCareHeightDataset",
            "version": "1.0",
            "type": "dataset_def"
        }
    ],
    "sources": [
        {
            "table": "etl.flat_hiv_summary_v15b",
            "alias": "fhs"
        },
        {
            "table": "amrs.patient_program",
            "alias": "pp_dc",
            "join": {
                "type": "LEFT",
                "joinCondition": "pp_dc.patient_id = fhs.person_id AND pp_dc.program_id in (3,9)  and  (pp_dc.date_completed is null || date(pp_dc.date_completed) >= date('{startDate}')) and  date(pp_dc.date_enrolled) <= date('{endDate}')"
            }
        },
        {
            "table": "amrs.patient_program",
            "alias": "pp_viremia",
            "join": {
                "type": "left",
                "joinCondition": "fhs.person_id = pp_viremia.patient_id AND pp_viremia.program_id = 27 AND (pp_viremia.date_completed is null || date(pp_viremia.date_completed) <= date('{endDate}'))"
            }
        },
        {
            "table": "amrs.cohort_member",
            "alias": "cm",
            "join": {
                "type": "left",
                "joinCondition": "cm.patient_id = fhs.person_id AND cm.voided = 0"
            }
        },
        {
            "table": "amrs.cohort",
            "alias": "ch",
            "join": {
                "type": "left",
                "joinCondition": "ch.cohort_id = cm.cohort_id"
            }
        },
        {
            "table": "amrs.person",
            "alias": "person",
            "join": {
                "type": "left",
                "joinCondition": "person.person_id = fhs.person_id"
            }
        },
        {
            "dataSet": "differentiatedCareWeightDataset",
            "alias": "wd",
            "join": {
                "type": "LEFT",
                "joinCondition": "fhs.person_id = wd.person_id and fhs.location_id = wd.location_id"
            }
        },
        {
            "table": "etl.flat_vitals",
            "alias": "fv",
            "join": {
                "type": "left",
                "joinCondition": "fv.person_id = wd.person_id and fv.encounter_datetime = wd.latest_encounter_date and fv.location_id = wd.location_id"
            }
        },
        {
            "dataSet": "differentiatedCareHeightDataset",
            "alias": "hd",
            "join": {
                "type": "LEFT",
                "joinCondition": "fhs.person_id = hd.person_id and fhs.location_id = hd.location_id"
            }
        },
        {
            "table": "etl.flat_vitals",
            "alias": "fvh",
            "join": {
                "type": "left",
                "joinCondition": "fvh.person_id = hd.person_id and fvh.encounter_datetime = hd.latest_encounter_date and fvh.location_id = hd.location_id"
            }
        }
    ],
    "columns": [
        {
            "type": "simple_column",
            "alias": "persons_id",
            "column": "fhs.person_id"
        },
        {
            "type": "derived_column",
            "alias": "eligible_for_dc",
            "expressionType": "simple_expression",
            "expressionOptions": {
                "expression": "if(fhs.vl_1 < 401 and fhs.vl_2_date IS NOT NULL AND (TIMESTAMPDIFF(MONTH,fhs.tb_tx_start_date,date('{endDate}')) >=6 OR TIMESTAMPDIFF(MONTH,fhs.tb_tx_start_date,date('{endDate}')) IS NULL) and (timestampdiff(year,person.birthdate, date('{endDate}')) >= 20) and (TIMESTAMPDIFF(MONTH, fhs.ipt_completion_date,date('{endDate}')) >= 6 ) and (timestampdiff(month,fhs.vl_1_date,date('{endDate}')) <= 12 ) and (timestampdiff(month,fhs.arv_first_regimen_start_date, date('{endDate}')) >= 12) AND pp_dc.program_id IS NULL and DATE(fhs.prev_clinical_rtc_date_hiv) = DATE(fhs.encounter_datetime) and fhs.is_clinical_encounter = 1 and fhs.is_pregnant IS NULL and fv.weight/((fvh.height/100) * (fvh.height/100)) >= 18.5 , 1, NULL)"
            }
        },
        {
            "type": "derived_column",
            "alias": "enrolled_in_dc",
            "expressionType": "simple_expression",
            "expressionOptions": {
                "expression": "if(pp_dc.program_id = 9 and pp_dc.date_completed is null, 1, NULL)"
            }
        },
        {
            "type": "derived_column",
            "alias": "enrolled_in_dc_community",
            "expressionType": "simple_expression",
            "expressionOptions": {
                "expression": "if(pp_dc.program_id = 9 and pp_dc.date_completed is null and ch.cohort_type_id = 1, 1, NULL)"
            }
        },
        {
            "type": "derived_column",
            "alias": "enrolled_in_dc_active",
            "expressionType": "simple_expression",
            "expressionOptions": {
                "expression": "if(pp_dc.program_id = 9 and pp_dc.date_completed is null, 1, NULL)"
            }
        },
        {
            "type": "simple_column",
            "alias": "vl_1",
            "column": "fhs.vl_1"
        },
        {
            "type": "simple_column",
            "alias": "encounter_datetime",
            "column": "fhs.encounter_datetime"
        },
        {
            "type": "simple_column",
            "alias": "vl_1_date",
            "column": "fhs.vl_1_date"
        },
        {
            "type": "simple_column",
            "alias": "cur_arv_meds",
            "column": "fhs.cur_arv_meds"
        },
        {
            "type": "simple_column",
            "alias": "height",
            "column": "fvh.height"
        },
        {
            "type": "simple_column",
            "alias": "weight",
            "column": "fv.weight"
        },
        {
            "type": "simple_column",
            "alias": "prev_rtc_date",
            "column": "fhs.prev_rtc_date"
        },
        {
            "type": "simple_column",
            "alias": "bmi",
            "column": "fv.weight/((fvh.height/100) * (fvh.height/100))"
        }
    ],
    "filters": {
        "conditionJoinOperator": "and",
        "conditions": [
            {
                "filterType": "tableColumns",
                "conditionExpression": "date(fhs.encounter_datetime) <= ?",
                "parameterName": "endDate"
            },
            {
                "filterType": "tableColumns",
                "conditionExpression": "Timestampdiff(day, fhs.rtc_date, ?) <= 28",
                "parameterName": "endDate"
            },
            {
                "filterType": "tableColumns",
                "conditionExpression": "fhs.location_uuid in (?)",
                "parameterName": "locationUuids"
            },
            {
                "filterType": "tableColumns",
                "conditionExpression": "fhs.vl_1 < 401",
                "parameterName": ""
            },
            {
                "filterType": "tableColumns",
                "conditionExpression": "fhs.encounter_type NOT IN (99999)",
                "parameterName": ""
            },
            {
                "filterType": "tableColumns",
                "conditionExpression": "fhs.death_date IS NULL",
                "parameterName": ""
            },
            {
                "filterType": "tableColumns",
                "conditionExpression": "fhs.transfer_out IS NULL",
                "parameterName": ""
            },
            {
                "filterType": "tableColumns",
                "conditionExpression": "fhs.rtc_date > ? ",
                "parameterName": "endDate"
            }
        ]
    }
}