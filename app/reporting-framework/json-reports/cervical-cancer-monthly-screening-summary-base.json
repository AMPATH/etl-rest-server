{
  "name": "cervicalCancerMonthlyReportBase",
  "version": "1.0",
  "tag": "",
  "uses": [],
  "sources": [
    {
      "table": "etl.flat_cervical_cancer_screening_test2",
      "alias": "fccs"
    },
    {
      "table": "amrs.person_name",
      "alias": "p2",
      "join": {
        "type": "INNER",
        "joinCondition": "fccs.person_id = p2.person_id AND (p2.voided is NULL || p2.voided = 0)"
      }
    },
    {
      "table": "amrs.patient_identifier",
      "alias": "p3",
      "join": {
        "type": "LEFT OUTER",
        "joinCondition": "fccs.person_id = p3.patient_id AND (p3.voided is NULL || p3.voided = 0)"
      }
    },
    {
      "table": "amrs.person",
      "alias": "p5",
      "join": {
        "type": "INNER",
        "joinCondition": "fccs.person_id = p5.person_id AND (p5.voided is NULL || p5.voided = 0)"
      }
    },
    {
      "table": "amrs.location",
      "alias": "l",
      "join": {
        "type": "INNER",
        "joinCondition": "l.location_id = fccs.location_id"
      }
    },
    {
      "table": "amrs.person_attribute",
      "alias": "p4",
      "join": {
        "type": "LEFT",
        "joinCondition": "fccs.person_id = p4.person_id AND (p4.voided is NULL|| p4.voided = 0 and (p4.person_attribute_type_id =10))"
      }
    }
  ],
  "columns": [
    {
      "type": "simple_column",
      "alias": "person_id",
      "column": "fccs.person_id"
    },
    {
      "type": "derived_column",
      "alias": "person_name",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "CONCAT(COALESCE(p2.given_name, ''), ' ', COALESCE(p2.middle_name, ''), ' ', COALESCE(p2.family_name, ''))"
      }
    },
    {
      "type": "simple_column",
      "alias": "phone_number",
      "column": "p4.value"
    },
    {
      "type": "derived_column",
      "alias": "identifiers",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "GROUP_CONCAT(DISTINCT p3.identifier SEPARATOR ', ')"
      }
    },
    {
      "type": "derived_column",
      "alias": "cur_visit_type",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "caseOptions": [
          {
            "condition": "cur_visit_type = 1",
            "value": "Scheduled visit"
          },
          {
            "condition": "cur_visit_type = 2",
            "value": "Unscheduled visit early"
          },
          {
            "condition": "cur_visit_type = 3",
            "value": "Unscheduled visit late"
          }
        ]
      }
    },
    {
      "type": "derived_column",
      "alias": "reasons_for_current_visit",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "caseOptions": [
          {
            "condition": "reasons_for_current_visit = 1",
            "value": "Initial screening"
          },
          {
            "condition": "reasons_for_current_visit = 2",
            "value": "Routine screening"
          },
          {
            "condition": "reasons_for_current_visit = 3",
            "value": "Post-treatment screening"
          },
          {
            "condition": "reasons_for_current_visit = 4",
            "value": "Treatment visit"
          },
          {
            "condition": "reasons_for_current_visit = 5",
            "value": "Complications"
          }
        ]
      }
    },
    {
      "type": "derived_column",
      "alias": "initial_screenings",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.reasons_for_current_visit = 1, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "routine_screenings",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.reasons_for_current_visit = 2, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "post_treatment_screenings",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.reasons_for_current_visit = 3, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "screening_method",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "caseOptions": [
          {
            "condition": "screening_method = 1",
            "value": "VIA or VIA/VILI"
          },
          {
            "condition": "screening_method = 3",
            "value": "Pap smear"
          },
          {
            "condition": "screening_method = 4",
            "value": "HPV"
          }
        ]
      }
    },
    {
      "type": "derived_column",
      "alias": "via_or_via_vili_test_result",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "caseOptions": [
          {
            "condition": "via_or_via_vili_test_result = 1",
            "value": "Negative"
          },
          {
            "condition": "via_or_via_vili_test_result = 2",
            "value": "Positive"
          },
          {
            "condition": "via_or_via_vili_test_result = 3",
            "value": "Suspicious of cancer"
          }
        ]
      }
    },
    {
      "type": "derived_column",
      "alias": "observations_from_positive_via_or_via_vili_test",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "caseOptions": [
          {
            "condition": "observations_from_positive_via_or_via_vili_test = 7472",
            "value": "Abnormal vessels"
          },
          {
            "condition": "observations_from_positive_via_or_via_vili_test = 7469",
            "value": "Aceto white lesions"
          },
          {
            "condition": "observations_from_positive_via_or_via_vili_test = 6497",
            "value": "Excessive bleeding"
          },
          {
            "condition": "observations_from_positive_via_or_via_vili_test = 9593",
            "value": "Friable tissue"
          },
          {
            "condition": "observations_from_positive_via_or_via_vili_test = 7470",
            "value": "Punctuated capillaries"
          },
          {
            "condition": "observations_from_positive_via_or_via_vili_test = 7293",
            "value": "Ulceration"
          },
          {
            "condition": "observations_from_positive_via_or_via_vili_test = 11772",
            "value": "Yellow staining lesion"
          }
        ]
      }
    },
    {
      "type": "derived_column",
      "alias": "hpv_test_result",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "caseOptions": [
          {
            "condition": "hpv_test_result = 1",
            "value": "Negative"
          },
          {
            "condition": "hpv_test_result = 2",
            "value": "Positive"
          },
          {
            "condition": "hpv_test_result = 3",
            "value": "Unknown"
          }
        ]
      }
    },
    {
      "type": "derived_column",
      "alias": "hpv_type",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "caseOptions": [
          {
            "condition": "hpv_type = 1",
            "value": "HPV TYPE 16"
          },
          {
            "condition": "hpv_type = 2",
            "value": "HPV TYPE 18"
          },
          {
            "condition": "hpv_type = 3",
            "value": "HPV TYPE 45"
          }
        ]
      }
    },
    {
      "type": "derived_column",
      "alias": "pap_smear_test_result",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "caseOptions": [
          {
            "condition": "pap_smear_test_result = 1",
            "value": "Negative"
          },
          {
            "condition": "pap_smear_test_result = 2",
            "value": "Positive"
          },
          {
            "condition": "pap_smear_test_result = 3",
            "value": "Suspicious of cancer"
          }
        ]
      }
    },
    {
      "type": "derived_column",
      "alias": "treatment_method",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "caseOptions": [
          {
            "condition": "treatment_method = 1",
            "value": "None"
          },
          {
            "condition": "treatment_method = 2",
            "value": "Cryotherapy"
          },
          {
            "condition": "treatment_method = 3",
            "value": "LEEP"
          },
          {
            "condition": "treatment_method = 4",
            "value": "Thermocoagulation"
          },
          {
            "condition": "treatment_method = 5",
            "value": "Other"
          }
        ]
      }
    },
    {
      "type": "derived_column",
      "alias": "procedures_done_this_visit",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "caseOptions": [
          {
            "condition": "procedures_done_this_visit = 1",
            "value": "None"
          },
          {
            "condition": "procedures_done_this_visit = 2",
            "value": "Endometrial biopsy"
          },
          {
            "condition": "procedures_done_this_visit = 3",
            "value": "Punch biopsy"
          },
          {
            "condition": "procedures_done_this_visit = 4",
            "value": "Polypectomy"
          }
        ]
      }
    },
    {
      "type": "simple_column",
      "alias": "age",
      "column": "fccs.age"
    },
    {
      "type": "simple_column",
      "alias": "gender",
      "column": "p5.gender"
    },
    {
      "type": "simple_column",
      "alias": "location_id",
      "column": "fccs.location_id"
    },
    {
      "type": "simple_column",
      "alias": "location_uuid",
      "column": "fccs.location_uuid"
    },
    {
      "type": "simple_column",
      "alias": "location_name",
      "column": "l.name"
    },
    {
      "type": "simple_column",
      "alias": "encounter_datetime",
      "column": "DATE_FORMAT(fccs.encounter_datetime, '%Y-%m-%d')"
    },
    {
      "type": "derived_column",
      "alias": "hiv_status",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "case when hiv_status=1 then 'HIV Negative' when hiv_status=2 then 'HIV Positive' when hiv_status=3 then 'Unknown' else NULL end"
      }
    },
    {
      "type": "derived_column",
      "alias": "prior_via_done",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "CASE WHEN prior_via_done = 1 THEN 'Yes' WHEN prior_via_done = 0 THEN 'No' ELSE NULL END"
      }
    },
    {
      "type": "simple_column",
      "alias": "prior_via_date",
      "column": "fccs.prior_via_date"
    },
    {
      "type": "derived_column",
      "alias": "prior_via_result",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "CASE WHEN prior_via_result = 1 THEN 'Positive' WHEN prior_via_result = 2 THEN 'Negative' ELSE NULL END"
      }
    },
    {
      "type": "derived_column",
      "alias": "cervix_impression",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "caseOptions": [
          {
            "condition": "visual_impression_cervix = 1",
            "value": "Normal"
          },
          {
            "condition": "visual_impression_cervix = 2",
            "value": "Positive VIA with Aceto white area"
          },
          {
            "condition": "visual_impression_cervix = 3",
            "value": "Positive VIA with suspicious lesion"
          }
        ]
      }
    },
    {
      "type": "derived_column",
      "alias": "vagina_impression",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "caseOptions": [
          {
            "condition": "visual_impression_vagina = 1",
            "value": "Normal"
          },
          {
            "condition": "visual_impression_vagina = 2",
            "value": "Warts, Genital"
          },
          {
            "condition": "visual_impression_vagina = 3",
            "value": "Suspicious of cancer, vaginal lesion"
          }
        ]
      }
    },
    {
      "type": "derived_column",
      "alias": "vulva_impression",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "caseOptions": [
          {
            "condition": "visual_impression_vulva = 1",
            "value": "Normal"
          },
          {
            "condition": "visual_impression_vulva = 2",
            "value": "Warts, Genital"
          },
          {
            "condition": "visual_impression_vulva = 3",
            "value": "Suspicious of cancer, vulva lesion"
          }
        ]
      }
    },
    {
      "type": "derived_column",
      "alias": "total_procedures_done",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(procedures_done_this_visit != 1, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "endometrial_biopsies_done",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(procedures_done_this_visit = 2, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "punch_biopsies_done",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(procedures_done_this_visit = 3, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "polypectomies_done",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(procedures_done_this_visit = 4, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "total_via_or_via_vili_tests",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(screening_method = 1, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "total_pap_smear_tests",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(screening_method = 3, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "total_hpv_tests",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(screening_method = 4, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "follow_up_plan",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "caseOptions": [
          {
            "condition": "follow_up_plan = 1",
            "value": "Return for results"
          },
          {
            "condition": "follow_up_plan = 2",
            "value": "VIA follow-up in 3 to 6 months"
          },
          {
            "condition": "follow_up_plan = 3",
            "value": "Routine yearly VIA"
          },
          {
            "condition": "follow_up_plan = 4",
            "value": "Routine 3 year VIA"
          },
          {
            "condition": "follow_up_plan = 5",
            "value": "Colposcopy planned"
          },
          {
            "condition": "follow_up_plan = 6",
            "value": "Gynecologic oncology services"
          }
        ]
      }
    },
    {
      "type": "simple_column",
      "alias": "screening_rtc_date",
      "column": "DATE_FORMAT(fccs.screening_rtc_date, '%Y-%m-%d')"
    },
    {
      "type": "derived_column",
      "alias": "positive_via_or_via_vili",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.via_or_via_vili_test_result = 2, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "positive_hpv",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.hpv_test_result = 2, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "positive_cytology",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.pap_smear_test_result IN (2,3,4,5,6), 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "total_treated_via_cryo",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.treatment_method = 2, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "treated_via_cryo_under_25",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.treatment_method = 2 AND age < 25, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "treated_via_cryo_25_to_49",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.treatment_method = 2 AND (age >= 25 AND age <= 49), 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "treated_via_cryo_50_and_above",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.treatment_method = 2 AND age >= 50, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "total_treated_via_leep",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.treatment_method = 3, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "treated_via_leep_under_25",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.treatment_method = 3 AND age < 25, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "treated_via_leep_25_to_49",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.treatment_method = 3 AND (age >= 25 AND age <= 49), 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "treated_via_leep_50_and_above",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.treatment_method = 3 AND age >= 50, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "total_treated_via_thermo",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.treatment_method = 4, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "treated_via_thermo_under_25",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.treatment_method = 4 AND age < 25, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "treated_via_thermo_25_to_49",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.treatment_method = 4 AND (age >= 25 AND age <= 49), 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "treated_via_thermo_50_and_above",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.treatment_method = 4 AND age >= 50, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "total_treated_using_other_treatment_options",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.treatment_method = 5, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "treated_using_other_treatment_options_under_25",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.treatment_method = 5 AND age < 25, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "treated_using_other_treatment_options_25_to_49",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.treatment_method = 5 AND (age >= 25 or age <= 49), 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "treated_using_other_treatment_options_50_and_above",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.treatment_method = 5 AND age >= 50, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "cryotherapy_sva",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.treatment_method = 2 AND fccs.cryotherapy_status = 2, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "cryotherapy_postponed",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.treatment_method = 2 AND fccs.cryotherapy_status = 3, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "thermocoagulation_sva",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.treatment_method = 4 AND fccs.thermocoagulation_status = 2, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "thermocoagulation_postponed",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.treatment_method = 4 AND fccs.thermocoagulation_status = 3, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "leep_sva",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.treatment_method = 3 AND fccs.leep_status = 1, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "leep_postponed",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.treatment_method = 3 AND fccs.leep_status = 2, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "hiv_negative",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.hiv_status = 1, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "hiv_status_unknown",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.hiv_status = 3, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "hiv_positive",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.hiv_status = 2, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "hiv_positive_below_25",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.hiv_status = 2 and age < 25, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "hiv_negative_below_25",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.hiv_status = 1 and age < 25, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "hiv_unknown_below_25",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.hiv_status = 3 and age < 30, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "hiv_positive_25_to_49",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(fccs.hiv_status = 2 and age >= 25 and age < 50, 1, NULL)"
