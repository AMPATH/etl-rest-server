{
  "name": "txmlReportBase",
  "version": "1.0",
  "tag": "",
  "description": "",
  "uses": [],
  "sources": [
    {
      "table": "etl.hiv_monthly_report_dataset_frozen",
      "alias": "hm"
    },
    {
      "table": "amrs.location",
      "alias": "ll",
      "join": {
        "type": "INNER",
        "joinCondition": "hm.location_id = ll.location_id"
      }
    }
  ],
  "columns": [
    {
      "type": "simple_column",
      "alias": "person_id",
      "column": "hm.person_id"
    },
    {
      "type": "simple_column",
      "alias": "location_id",
      "column": "hm.location_id"
    },
    {
      "type": "simple_column",
      "alias": "location",
      "column": "ll.name"
    },
    {
      "type": "simple_column",
      "alias": "location_uuid",
      "column": "ll.uuid"
    },
    {
      "type": "simple_column",
      "alias": "status",
      "column": "hm.status"
    },
    {
      "type": "simple_column",
      "alias": "gender",
      "column": "hm.gender"
    },
    {
      "type": "simple_column",
      "alias": "age",
      "column": "hm.age"
    },
    {
      "type": "simple_column",
      "alias": "endDate",
      "column": "hm.endDate"
    },
    {
      "type": "derived_column",
      "alias": "dead_male_30_to_34",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if((hm.status = 'Dead' AND hm.gender = 'M' AND hm.age BETWEEN 30 AND 34),1,null)"
      }
    },
    {
      "type": "derived_column",
      "alias": "dead_female_30_to_34",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if((hm.status = 'Dead' AND hm.gender = 'F' AND hm.age BETWEEN 30 AND 34),1,null)"
      }
    },
    {
      "type": "derived_column",
      "alias": "dead_male_35_to_39",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if((hm.status = 'Dead' AND hm.gender = 'M' AND hm.age BETWEEN 35 AND 39),1,null)"
      }
    },
    {
      "type": "derived_column",
      "alias": "dead_female_35_to_39",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if((hm.status = 'Dead' AND hm.gender = 'F' AND hm.age BETWEEN 35 AND 39),1,null)"
      }
    }
  ],
  "filters": {
    "conditionJoinOperator": "and",
    "conditions": [
      {
        "filterType": "tableColumns",
        "conditionExpression": "hm.location_id IN ?",
        "parameterName": "locations"
      },
      {
        "filterType": "tableColumns",
        "conditionExpression": "hm.endDate = ?",
        "parameterName": "endDate"
      }
    ]
  },
  "groupBy": {
    "groupParam": "groupByParam",
    "columns": ["person_id"]
  }
}
