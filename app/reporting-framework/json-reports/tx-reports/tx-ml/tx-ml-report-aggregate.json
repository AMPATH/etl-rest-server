    {
      "type": "derived_column",
      "alias": "dead_male_30_to_34",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "COUNT(CASE WHEN b.status = 'Dead' AND b.gender = 'M' AND b.age BETWEEN 30 AND 34 THEN 1 ELSE NULL END)"
      }
    },
    {
      "type": "derived_column",
      "alias": "dead_female_30_to_34",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "COUNT(CASE WHEN b.status = 'Dead' AND b.gender = 'F' AND b.age BETWEEN 30 AND 34 THEN 1 ELSE NULL END)"
      }
    },
    {
      "type": "derived_column",
      "alias": "dead_male_35_to_39",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "COUNT(CASE WHEN b.status = 'Dead' AND b.gender = 'M' AND b.age BETWEEN 35 AND 39 THEN 1 ELSE NULL END)"
      }
    },
    {
      "type": "derived_column",
      "alias": "dead_female_35_to_39",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "COUNT(CASE WHEN b.status = 'Dead' AND b.gender = 'F' AND b.age BETWEEN 35 AND 39 THEN 1 ELSE NULL END)"
      }
    }
  ],
  "groupBy": {
    "groupParam": "groupByParam",
    "columns": ["b.location_id"],
    "excludeParam": "excludeParam"
  },
