{
  "name": "ayp731Base",
  "version": "1.0",
  "tag": "",
  "description": "",
  "uses": [],
  "sources": [
    {
      "table": "amrs.obs",
      "alias": "ob"
    },
    {
      "table": "amrs.person",
      "alias": "p",
      "join": {
        "type": "LEFT",
        "joinCondition": "ob.person_id = p.person_id"
      }
    },
    {
      "table": "etl.flat_patient_identifiers_v1",
      "alias": "flat_identifiers",
      "join": {
        "type": "LEFT",
        "joinCondition": "ob.person_id = flat_identifiers.patient_id"
      }
    },
    {
      "table": "amrs.encounter",
      "alias": "e",
      "join": {
        "type": "LEFT",
        "joinCondition": "ob.person_id = e.patient_id"
      }
    }
  ],
  "columns": [
    {
      "type": "simple_column",
      "alias": "person_id",
      "column": "ob.person_id"
    },
    {
      "type": "simple_column",
      "alias": "ccc_number",
      "column": "flat_identifiers.ccc"
    },
    {
      "type": "simple_column",
      "alias": "upi_number",
      "column": "flat_identifiers.nupi"
    },
    {
      "type": "derived_column",
      "alias": "age_10_19",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(TIMESTAMPDIFF(YEAR, p.birthdate, CURDATE()) BETWEEN 10 AND 19, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "age_20_24",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(TIMESTAMPDIFF(YEAR, p.birthdate, CURDATE()) BETWEEN 20 AND 24, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "known_positive_ayp",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(ob.concept_id = 1357  AND ob.value_coded = 10407, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "new_positive_ayp",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(ob.concept_id = 1357  AND ob.value_coded = 703, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "start_art",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(ob.concept_id = 1255  AND ob.value_coded = 1256, 1, NULL)"
      }
    },
    {
      "type": "derived_column",
      "alias": "on_art",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "if(ob.concept_id = 1255  AND ob.value_coded = 1257, 1, NULL)"
      }
    }
  ],
  "filters": {
    "conditionJoinOperator": "and",
    "conditions": [
      {
        "filterType": "tableColumns",
        "conditionExpression": "ob.concept_id in (12019, 1357, 1255)",
        "parameterName": ""
      },
      {
        "filterType": "tableColumns",
        "conditionExpression": "e.encounter_type = 32"
      },
      {
        "filterType": "tableColumns",
        "conditionExpression": "ob.obs_datetime >= ?",
        "parameterName": "startDate"
      },
      {
        "filterType": "tableColumns",
        "conditionExpression": "ob.obs_datetime <= ?",
        "parameterName": "endDate"
      },
      {
        "filterType": "tableColumns",
        "conditionExpression": "ob.location_id in ?",
        "parameterName": "locations"
      }
    ]
  },
  "groupBy": {
    "groupParam": "groupByParam",
    "columns": ["ob.person_id", "ob.concept_id"],
    "excludeParam": "excludeParam"
  }
}
