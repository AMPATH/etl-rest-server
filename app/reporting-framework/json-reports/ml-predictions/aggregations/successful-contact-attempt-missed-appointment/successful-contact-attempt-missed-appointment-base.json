{
  "name": "successfulContactMissedAppintmentBase",
  "version": "1.0",
  "tag": "",
  "description": "",
  "uses": [],
  "sources": [
    {
      "table": "predictions.ml_weekly_predictions",
      "alias": "ml"
    },
    {
      "table": "amrs.location",
      "alias": "al",
      "join": {
        "type": "INNER",
        "joinCondition": "ml.location_id = al.location_id"
      }
    },
    {
      "table": "etl.pre_appointment_summary",
      "alias": "pre",
      "join": {
        "type": "INNER",
        "joinCondition": "ml.person_id = pre.person_id AND DATE(pre.encounter_datetime) BETWEEN DATE(ml.prediction_generated_date) AND ml.start_date AND (pre.is_successful_phone_follow_up = 'YES' OR (pre.attempted_home_visit = 'YES' AND pre.was_client_found = 'YES'))"
      }
    },
    {
      "table": "etl.flat_patient_identifiers_v1",
      "alias": "flat_identifiers",
      "join": {
        "type": "LEFT",
        "joinCondition": "ml.person_id = flat_identifiers.patient_id"
      }
    }
  ],
  "columns": [
    {
      "alias": "location_name",
      "type": "simple_column",
      "column": "al.name"
    },
    {
      "alias": "person_id",
      "type": "simple_column",
      "column": "ml.person_id"
    },
    {
      "alias": "location_id",
      "type": "simple_column",
      "column": "ml.location_id"
    },
    {
      "type": "simple_column",
      "alias": "ccc_number",
      "column": "flat_identifiers.ccc"
    },
    {
      "type": "simple_column",
      "alias": "upi_number",
      "column": "flat_identifiers.nupi"
    },
    {
      "type": "simple_column",
      "alias": "cr_id",
      "column": "flat_identifiers.cr_id"
    },
    {
      "type": "simple_column",
      "alias": "sha_id",
      "column": "flat_identifiers.sha_id"
    },
    {
      "type": "derived_column",
      "alias": "q_num",
      "expressionType": "simple_expression",
      "expressionOptions": {
        "expression": "1"
      }
    }
  ],
  "filters": {
    "conditionJoinOperator": "and",
    "conditions": [
      {
        "filterType": "tableColumns",
        "conditionExpression": "ml.predicted_risk IS NOT NULL"
      },
      {
        "filterType": "tableColumns",
        "conditionExpression": "DATE(ml.prediction_generated_date) >= ?",
        "parameterName": "startDate"
      },
      {
        "filterType": "tableColumns",
        "conditionExpression": "DATE(ml.prediction_generated_date) <= ?",
        "parameterName": "endDate"
      },
      {
        "filterType": "tableColumns",
        "conditionExpression": "ml.location_id = ?",
        "parameterName": "locations"
      },
      {
        "filterType": "tableColumns",
        "conditionExpression": "EXISTS (SELECT  MIN(DATE(t.encounter_datetime)) FROM etl.flat_hiv_summary_v15b t WHERE t.person_id = ml.person_id AND (DATE(t.prev_rtc_date) BETWEEN DATE(ml.prediction_generated_date) AND ml.end_date) AND DATE(t.encounter_datetime) > ml.rtc_date  group by t.person_id, DATE(t.encounter_datetime))"
      }
    ]
  },
  "groupBy": {
    "groupParam": "groupByParam",
    "columns": ["ml.location_id", "ml.person_id"],
    "excludeParam": "excludeParam"
  }
}
