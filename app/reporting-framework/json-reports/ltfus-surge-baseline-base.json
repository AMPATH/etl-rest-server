    },
    {
      "type": "simple_column",
      "alias": "arv_first_regimen",
      "column": "lsbr.arv_first_regimen"
    },
    {
      "type": "simple_column",
      "alias": "active_to_ltfu_count",
      "column": "lsbr.active_to_ltfu_count"
    },
    {
      "type": "simple_column",
