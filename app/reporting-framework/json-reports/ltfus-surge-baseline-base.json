{
  "name": "surgeBaselineReport",
  "version": "1.0",
  "tag": "",
  "description": "",
  "uses": [],
  "sources": [
    {
      "table": "etl.surge_weekly_report_dataset_frozen",
      "alias": "lsbr"
    },
    {
      "table": "amrs.location",
      "alias": "l",
      "join": {
        "type": "INNER",
        "joinCondition": "l.location_id = lsbr.baseline_location"
      }
    }
  ],
  "columns": [
    {
      "type": "simple_column",
      "alias": "person_id",
      "column": "lsbr.person_id"
    },
    {
      "type": "simple_column",
      "alias": "is_ltfu_surge_baseline",
      "column": "lsbr.is_ltfu_surge_baseline"
